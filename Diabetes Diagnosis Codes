select * from PME.Hedis_v2_codes_2021;
select distinct VALUE_SET_NAME from PME.Hedis_v2_codes_2021
order by 1;


select distinct CODE, code_system from PME.Hedis_v2_codes_2021
where VALUE_SET_NAME  in ('Diabetes Diagnosis')
order by 2, 1;

